local function callback(text)
    if text == "1" then
game:GetService("Players").LocalPlayer:Kick("Correct")
    elseif text == "fuck you" then
game:GetService("Players").LocalPlayer:Kick("Wrong")
    end
end

local bindableFunction= Instance.new("BindableFunction")

game.StarterGui:SetCore("SendNotification", {
    Title = "hankiwashere";
    Text = "Solve this: log10";
    Icon = "";
    Duration = "10";
    callback = bindableFunction;
    Button1 = "1";
    Button2 = "fuck you";
}
